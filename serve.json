{
  "public":"/dist",
  "redirects": [
    {
      "source": "/",
      "destination": "/dist"
    }
  ],
  "rewrites": [
    {
      "source": "lib/:file",
      "destination": "lib/:file.js"
    },
    {
      "source": "lib/types/helpers/:file",
      "destination": "lib/types/helpers/:file.js"
    },
    {
      "source": "lib/types/modules/:file",
      "destination": "lib/types/modules/:file.js"
    }
  ]
}